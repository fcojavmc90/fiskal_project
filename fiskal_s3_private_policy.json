{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucketCases",
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::fiskal-documents-private91f6c-dev"],
      "Condition": {
        "StringLike": { "s3:prefix": ["cases/*"] }
      }
    },
    {
      "Sid": "ObjectAccessCases",
      "Effect": "Allow",
      "Action": ["s3:GetObject", "s3:PutObject", "s3:DeleteObject"],
      "Resource": ["arn:aws:s3:::fiskal-documents-private91f6c-dev/cases/*"]
    }
  ]
}
